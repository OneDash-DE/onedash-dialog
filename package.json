{
	"name": "onedash-dialog",
	"version": "1.2.2",
	"description": " OneDash. React dialog component",
	"author": "ChewbaccaCookie",
	"license": "MIT",
	"repository": "OneDash-DE/onedash-dialog",
	"main": "dist/index.js",
	"module": "dist/index.modern.js",
	"source": "src/index.ts",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "microbundle --no-sourcemap --css-modules false --jsx React.createElement --format modern",
		"start": "npm run storybook",
		"lint": "tsc --noEmit && npx eslint src/**/*.{js,ts,tsx} --quiet --fix",
		"format": "npx prettier --write src/**/*.{js,ts,tsx}",
		"release": "npx semantic-release",
		"preinstall": "node -e \"!process.env.npm_config_user_agent.startsWith('npm/') && !console.log('Use npm to install dependencies in this repository\\n') && process.exit(1)\"",
		"install-all": "npm i && cd example && npm i",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook -o example"
	},
	"peerDependencies": {
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-scripts": "^4.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/plugin-syntax-nullish-coalescing-operator": "^7.8.3",
		"@semantic-release/git": "^9.0.0",
		"@storybook/addon-actions": "^6.1.11",
		"@storybook/addon-essentials": "^6.1.11",
		"@storybook/addon-links": "^6.1.11",
		"@storybook/client-api": "^6.1.11",
		"@storybook/node-logger": "^6.1.14",
		"@storybook/preset-create-react-app": "^3.1.5",
		"@storybook/react": "^6.1.11",
		"@types/body-scroll-lock": "^2.6.1",
		"@types/node": "^14.14.16",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.12.0",
		"@typescript-eslint/parser": "^4.12.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.2",
		"conventional-changelog-conventionalcommits": "^4.5.0",
		"cross-env": "^7.0.3",
		"eslint": "^7.17.0",
		"eslint-config-airbnb-typescript": "^12.0.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-prettier": "^3.3.0",
		"husky": "^5.0.9",
		"microbundle": "^0.13.0",
		"prettier": "^2.2.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-scripts": "^4.0.1",
		"semantic-release": "^17.3.1",
		"typescript": "^4.1.3"
	},
	"files": [
		"dist"
	],
	"eslintConfig": {
		"extends": [
			"react-app"
		]
	},
	"husky": {
		"hooks": {
			"pre-merge": "npm run install-all"
		}
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"dependencies": {
		"body-scroll-lock": "^3.1.5",
		"sass": "^1.32.8"
	}
}
