{
	"name": "onedash-dialog",
	"version": "1.2.23",
	"description": " OneDash. React dialog component",
	"author": "ChewbaccaCookie",
	"license": "MIT",
	"repository": "OneDash-DE/onedash-dialog",
	"main": "dist/index.js",
	"module": "dist/index.modern.js",
	"source": "src/index.ts",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "microbundle --no-sourcemap --css-modules false --jsx React.createElement --format modern",
		"start": "npm run storybook",
		"lint": "tsc --noEmit && npx eslint src/** --quiet --fix",
		"format": "npx prettier --write src/**/*.{js,ts,tsx}",
		"release": "npx semantic-release",
		"install-all": "npm i && cd example && npm i",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook -o example"
	},
	"peerDependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": ">=4.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.16.5",
		"@babel/plugin-syntax-nullish-coalescing-operator": "^7.8.3",
		"@semantic-release/git": "^10.0.1",
		"@storybook/addon-actions": "^6.4.9",
		"@storybook/addon-essentials": "^6.4.9",
		"@storybook/addon-links": "^6.4.9",
		"@storybook/client-api": "^6.4.9",
		"@storybook/node-logger": "^6.4.9",
		"@storybook/preset-create-react-app": "^4.0.0",
		"@storybook/react": "^6.4.9",
		"@types/body-scroll-lock": "^3.1.0",
		"@types/node": "^17.0.1",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"@types/uuid": "^8.3.3",
		"@typescript-eslint/eslint-plugin": "^5.7.0",
		"@typescript-eslint/parser": "^5.7.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.3",
		"conventional-changelog-conventionalcommits": "^4.6.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.5.0",
		"eslint-config-airbnb": "^19.0.2",
		"eslint-config-airbnb-typescript": "^16.1.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"microbundle": "^0.14.2",
		"prettier": "^2.5.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "^5.0.0",
		"semantic-release": "^18.0.1",
		"typescript": "^4.5.4"
	},
	"files": [
		"dist"
	],
	"eslintConfig": {
		"extends": [
			"react-app"
		]
	},
	"husky": {
		"hooks": {
			"pre-merge": "npm run install-all"
		}
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"dependencies": {
		"body-scroll-lock": "^3.1.5",
		"sass": "^1.45.0"
	}
}
